#include<iostream>
#include<vector>
using namespace std;
vector<int> product(int array[],int  Size)
{
    vector<int> answer(Size, 1);
    for(int i=0 ; i < Size ; i++)
    {
     int  product_ = 1;
        for(int j=0 ; j < Size ; j++)
        {
            if (i != j)
            { 
               product_ *= array[j];
            }
        }
        answer[i] = product_;
    }
    return answer;

}
int main()
{
    cout << "Array Multiplication...." << endl;
    int array[] = {1, 2, 3, 4};
    int Size = sizeof(array)/ sizeof(int);
    vector<int> answer =  product(array, Size);
    for(int i=0 ; i < Size ; i++)
    {
        cout << answer[i]<<" ";
    }
    return 0;
}