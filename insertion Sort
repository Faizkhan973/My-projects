#include<iostream>
#include<vector>
using namespace std;
void Insertion_Sort(int array[], int n)
{
    for (int i=1 ; i < n; i++)
    {
        int store_value = array[i];
        int j= i-1;
        while(j>= 0 && array[j] > store_value)
        {
            array[j+1 ] = array[j];
            j--;
        }
        array[j+1] = store_value;
    }

}
int main()
{
    cout << "Insertion Sort ......." << endl;
    int array[] = {6,5,8,7,2,1,3};
    int n = sizeof(array)/ sizeof(array[0]);
    Insertion_Sort(array, n);
    for(int i=0 ; i < n ; i++)
    {
    cout << array[i]<<"  ";
    }
    return 0;
}