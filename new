int main() {
    double v1;
    double v2;
    cout << "inter radian of angle 1" << endl;
    cin >> v1;

    cout << "inter radian of angle 1" << endl;
    cin >>v2 ;

    double   x = sin(v1) + cos(v2);
    cout << "Required Result : " << x<< endl;
    return 0;
}